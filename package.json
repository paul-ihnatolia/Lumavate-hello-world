{
  "name": "react-express-docker-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for React-Express app with testing framework and Docker integration",
  "main": "src/server/app.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon ./src/server/app.js --exec babel-node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MisaOgura/react-express-docker-boilerplate.git"
  },
  "keywords": [
    "javascript",
    "react",
    "es6",
    "express",
    "webpack",
    "babel",
    "docker",
    "jest"
  ],
  "author": "Misa Ogura",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MisaOgura/react-express-docker-boilerplate/issues"
  },
  "homepage": "https://github.com/MisaOgura/react-express-docker-boilerplate#readme",
  "dependencies": {
    "@lumavate/request-signer": "https://github.com/Lumavate-Team/js-signer",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "cross-env": "^5.0.5",
    "morgan": "^1.9.1",
    "multer": "^1.3.1",
    "prop-types": "^15.6.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-hot-loader": "^3.0.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "webpack": "^3.5.3",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.19.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.16",
    "concurrently": "^3.5.0",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "express": "^4.15.4",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^20.0.4",
    "nodemon": "^1.18.4",
    "react-test-renderer": "^16.0.0",
    "standard": "^10.0.3",
    "supertest": "^3.0.0"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "dist/"
    ],
    "setupFiles": [
      "<rootDir>/src/shim.js",
      "<rootDir>/src/setupTest.js"
    ]
  },
  "standard": {
    "globals": [
      "describe",
      "context",
      "before",
      "beforeEach",
      "after",
      "afterEach",
      "it",
      "expect"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      ""
    ]
  }
}
